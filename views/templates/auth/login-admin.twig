    </head>

    <body style="background:url('http://www.myalivechurch.org/wp-content/uploads/2015/05/background_blue_v0011.jpg')">

        {% include 'templates/partials/flash.twig' %}
 
<div class="login-box">
  <div class="login-logo">
    <a href="{{path_for('login.admin')}}"><b>Login</b>&nbsp;Admin</a>
  </div>
  <!-- /.login-logo -->
  <div class="login-box-body">
  <img style="width:350px; background:url('http://www.myalivechurch.org/wp-content/uploads/2015/05/background_blue_v0011.jpg')" src="http://www.timescapetech.co.ke/img/user2.png" class="img-thumbnail" alt="">
    <p class="login-box-msg"><b>Sign in to Admin</b></p>

    <form action="{{path_for('login.admin')}}" method="post">
      <div class="form-group has-feedback">
        <input type="text" class="form-control" name="username" placeholder="Username...">
        <span class="fa fa-users form-control-feedback"></span>
      </div>
      <div class="form-group has-feedback">
        <input type="password" class="form-control" name="password" placeholder="Password">
        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
      </div>
      <div class="row">
        <div class="col-xs-4">
          <button type="submit" class="btn btn-social-button btn-yahoo">Sign In</button>
        </div>
        <!-- /.col -->
      </div>
    </form>
</div>
  <!-- /.login-box-body -->
</div>

<script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>         
